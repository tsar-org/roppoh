{
  "name": "roppoh",
  "private": true,
  "type": "module",
  "engines": {
    "node": "22",
    "pnpm": "9.15",
    "npm": "please_use_pnpm_instead"
  },
  "scripts": {
    "prepare": "lefthook install",
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "pages:build": "pnpm next-on-pages",
    "preview": "pnpm pages:build && wrangler pages dev",
    "deploy": "pnpm pages:build && wrangler pages deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "tsc": "tsc --noEmit",
    "format": "pnpm biome check --fix --unsafe",
    "fix:next-lint": "next lint --fix",
    "check:next-lint": "next lint",
    "check:biome": "pnpm biome check",
    "check:editorconfig": "editorconfig-checker",
    "bundle-analyze": "ANALYZE=true pnpm run build",
    "pre-commit": "lefthook run pre-commit --all-files"
  },
  "dependencies": {
    "@discord/embedded-app-sdk": "^1.9.0",
    "@hono/valibot-validator": "^0.5.2",
    "@radix-ui/react-dropdown-menu": "^2.1.5",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.0.6",
    "hono": "^4.6.20",
    "lucide-react": "^0.474.0",
    "next": "15.1.5",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-unity-webgl": "^9.7.0",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "valibot": "1.0.0-beta.14"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@cloudflare/next-on-pages": "1",
    "@cloudflare/workers-types": "^4.20250129.0",
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9.19.0",
    "@next/bundle-analyzer": "^15.1.6",
    "@types/node": "^20.17.16",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "editorconfig-checker": "^6.0.1",
    "eslint": "^9",
    "eslint-config-biome": "^1.9.4",
    "eslint-config-next": "15.1.5",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-next-on-pages": "^1.13.7",
    "lefthook": "^1.10.10",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vercel": "^39.4.2",
    "wrangler": "^3.107.0"
  },
  "packageManager": "pnpm@9.15.4"
}
