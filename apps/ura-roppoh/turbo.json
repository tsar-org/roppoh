{
  "$schema": "https://turborepo.com/schema.json",
  "extends": ["//"],
  "tasks": {
    "build": {
      "inputs": ["src", "worker", "./*.ts"],
      "outputs": ["./dist/**"]
    },
    "cf-typegen": {
      "inputs": ["wrangler.jsonc"],
      "outputs": ["worker-configuration.d.ts"]
    },
    "check:type": {
      "inputs": [
        "src",
        "worker",
        "$TURBO_ROOT$/tsconfig.json",
        "./*.ts",
        "./tsconfig.json"
      ]
    }
  }
}
