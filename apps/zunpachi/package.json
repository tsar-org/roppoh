{
  "dependencies": {
    "@roppoh/better-auth": "workspace:*",
    "better-auth": "^1.4.10",
    "hono": "^4.11.0",
    "inversify": "^7.10.8",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.17.1",
    "vite": "^7.2.7",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16",
    "wrangler": "^4.54.0"
  },
  "main": "./src/entry.ts",
  "name": "@roppoh/zunpachi",
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "private": true,
  "scripts": {
    "build": "vite build",
    "cf-typegen": "wrangler types  --strict-vars=false",
    "check:type": "tsc --noEmit",
    "deploy": "bun run build && wrangler deploy",
    "dev": "bun run reset:wrangler && bun run migration && bun run seed && bun vite",
    "migration": "wrangler d1 migrations apply roppoh-better-auth --local",
    "reset:wrangler": "rm -rf .wrangler",
    "seed": "wrangler d1 execute roppoh-better-auth --local --file='../../packages/better-auth/src/seeder/super-admin.sql'",
    "test": "vitest run"
  },
  "type": "module",
  "version": "0.0.0"
}
