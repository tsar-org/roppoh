{
  "$schema": "https://turborepo.com/schema.json",
  "extends": ["//"],
  "tasks": {
    "build": {
      "inputs": ["src", "./*.ts"],
      "outputs": ["build/**"]
    },
    "cf-typegen": {
      "inputs": ["wrangler.jsonc"],
      "outputs": ["worker-configuration.d.ts"]
    },
    "check:type": {
      "inputs": [
        "src",
        "./*.ts",
        "$TURBO_ROOT$/tsconfig.json",
        "./tsconfig.json"
      ],
      "outputs": [".react-router/types/**"]
    }
  }
}
