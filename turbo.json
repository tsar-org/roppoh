{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "cache": true,
      "dependsOn": ["^build"]
    },
    "cf-typegen": {
      "cache": true,
      "dependsOn": ["^build"],
      "outputs": ["worker-configuration.d.ts"]
    },
    "check:type": {
      "cache": true,
      "dependsOn": ["^build"],
      "outputs": []
    },
    "test": {
      "cache": false
    }
  }
}
